{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/"
      ],
      "@primePreset/*": [
        "src/primePreset/*"
      ],
      "@router/*": [
        "src/router/*"
      ],
      "@types/*": [
        "src/types/*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "include": [
      "src/**/*.ts",
      "src/**/*.d.ts",
      "src/**/*.vue",
      "src/**/*.tsx",
      "shims-vue.d.ts"
    ]
  }
}
